pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'building'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        stage('greeting') {
            steps {
                echo 'Nice to meet you'
            }
        }
        stage('test') {
            steps {
                echo 'Teting'
            }
        }
    }
}
